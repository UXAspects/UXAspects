{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "documentation": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "less"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "",
            "sourceRoot": "docs",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "outputs": ["{options.outputPath}"],
                    "options": {
                        "customWebpackConfig": {
                            "path": "./docs/webpack.config.ts"
                        },
                        "outputPath": "dist/docs",
                        "index": "docs/index.html",
                        "main": "docs/main.ts",
                        "polyfills": "docs/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "less",
                        "assets": [
                            "docs/favicon.ico",
                            "docs/assets",
                            {
                                "glob": "**/*",
                                "input": "src/fonts/",
                                "output": "/assets/fonts/"
                            },
                            {
                                "glob": "**/*",
                                "input": "src/img/",
                                "output": "/assets/img/"
                            },
                            {
                                "glob": "**/*",
                                "input": "dist/library/styles/",
                                "output": "/assets/css/"
                            },
                            {
                                "glob": "ux-aspects-ux-aspects.mjs",
                                "input": "dist/library/fesm2015",
                                "output": "/assets/lib"
                            },
                            {
                                "glob": "ux-aspects-ux-aspects.tgz",
                                "input": "target/npm",
                                "output": "/assets/lib"
                            }
                        ],
                        "styles": ["docs/styles.less"],
                        "scripts": [],
                        "allowedCommonJsDependencies": [
                            "chart.js",
                            "cheerio",
                            "codesandbox/lib/api/define",
                            "json-templater/object",
                            "typescript"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [],
                            "fileReplacements": [],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "documentation:build:production",
                            "port": 8080
                        },
                        "development": {
                            "browserTarget": "documentation:build:development",
                            "port": 8080
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
                    }
                }
            }
        },
        "ux-aspects-e2e": {
            "root": "",
            "sourceRoot": "e2e",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser-esbuild",
                    "options": {
                        "outputPath": "dist/e2e",
                        "index": "e2e/pages/index.html",
                        "main": "e2e/pages/main.ts",
                        "polyfills": "e2e/pages/polyfills.ts",
                        "tsConfig": "e2e/tsconfig.app.json",
                        "inlineStyleLanguage": "css",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "e2e/pages/assets",
                                "output": "/assets"
                            }
                        ],
                        "styles": [
                            "node_modules/source-sans-pro/source-sans-pro.css",
                            "node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "src/styles/ux-aspects.less",
                            "e2e/pages/styles.css"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [],
                            "fileReplacements": [],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "ux-aspects-e2e:build:production",
                            "port": 4000
                        },
                        "development": {
                            "browserTarget": "ux-aspects-e2e:build:development",
                            "port": 4000
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "./protractor.conf.js",
                        "webdriverUpdate": false
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
                    }
                }
            }
        },
        "ux-aspects": {
            "projectType": "library",
            "root": "",
            "sourceRoot": "src",
            "prefix": "ux",
            "architect": {
                "build": {
                    "builder": "@ashley-hunter/ng-packagr-compat:package",
                    "outputs": ["dist/library"],
                    "options": {
                        "project": "src/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "src/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "src/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "node_modules/bootstrap/dist/css/bootstrap.min.css",
                            "src/styles/ux-aspects.less"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
                    }
                }
            }
        }
    },
    "cli": {
        "schematicCollections": ["@angular-eslint/schematics"]
    }
}
