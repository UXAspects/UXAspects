    "scripts": {
        "ng": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng.js",
        "build:documentation": "grunt build:documentation",
        "build:library": "grunt build:library",
        "build": "grunt build",
        "docker:ci": "node ./scripts/docker.js",
        "generate:iconset": "node ./scripts/docker.js uxaspects/buildenv:1 \"npm install && npx grunt iconset\"",
        "lint": "grunt lint",
        "postpack": "mv src_package.json src/package.json",
        "prepack": "mv src/package.json src_package.json",
        "setversion": "ux-setversion package.json package-lock.json src/package.json",
        "start:e2e": "ng serve ux-aspects-e2e",
        "start:library": "npm run ng build ux-aspects -- --watch",
        "start:karma": "npm run ng test ux-aspects -- --watch --browsers=Chrome",
        "start:documentation": "npm run ng serve documentation",
        "start": "concurrently \"npm run start:documentation\" \"npm run start:library\" \"node ./scripts/serve-plunker.js\"",
        "test:e2e": "ng e2e ux-aspects-e2e",
        "test:karma": "npm run ng test ux-aspects -- --watch=false",
        "test": "npm run test:karma && npm run test:e2e",
        "format": "prettier --write"
    },
