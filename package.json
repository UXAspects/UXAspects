  "scripts": {
    "build:documentation": "grunt build:documentation",
    "build:library": "grunt build:library",
    "build": "npm run lint && grunt build",
    "docker:ci": "node ./scripts/docker.js",
    "format": "prettier --write \"**/*.{ts,js,json,css,less,html}\"",
    "generate:iconset": "node ./scripts/docker.js uxaspects/buildenv:1 \"npm install && npx grunt iconset\"",
    "lint": "nx run-many -t lint --parallel",
    "nx": "nx",
    "postpack": "mv src_package.json src/package.json",
    "prepack": "mv src/package.json src_package.json",
    "setversion": "ux-setversion package.json package-lock.json src/package.json",
    "start:documentation": "npm run nx serve documentation",
    "start:e2e": "nx serve ux-aspects-e2e",
    "start:karma": "npm run nx test ux-aspects -- --watch --browsers=Chrome",
    "start:library": "npm run nx build ux-aspects -- --watch",
    "start:pack": "node ./scripts/run-watch-pack.js",
    "start:style": "webpack watch --config configs/webpack.css-assets.dev.config --output-path dist/library/styles",
    "start": "rimraf dist/library && concurrently --restart-tries 3 --restart-after 5000 -n docs,lib,style,pack,serve -c cyan,magenta,yellow,bgCyan,bgMagenta \"npm:start:documentation\" \"npm:start:library\" \"npm:start:style\" \"npm:start:pack\" \"http-server target/playground -p 8090 --cors\"",
    "test:e2e": "nx e2e ux-aspects-e2e",
    "test:karma": "npm run nx test ux-aspects -- --watch=false",
    "test": "npm run test:karma && npm run test:e2e",
    "start:local-registry": "nx local-registry @ux-aspects/ux-aspects-docs",
    "publish:local-registry": "sh ./scripts/local-publish.sh"
  },
