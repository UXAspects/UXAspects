{
    "compilerOptions": {
      "target": "es5",
      "module": "es2015",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": ["es2017", "dom"],
      "noImplicitAny": true,
      "declaration": false,
      "suppressImplicitAnyIndexErrors": true
    },
    "angularCompilerOptions": {
      "skipMetadataEmit": true,
      "preserveWhitespaces": false
    }
  }
  